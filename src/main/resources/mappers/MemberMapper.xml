<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Member">


    <insert id="signup" parameterType="member">
        insert into member
        (memberEmail,memberPassword,memberNickname,memberGender,memberRegdate,memberPhonenumber,
        memberSchool,memberGradeCode,memberSubjectCode,memberRegionCode)
        values
        (#{memberEmail},#{memberPassword},#{memberNickname},#{memberGender},#{memberRegdate},#{memberPhonenumber},
        #{memberSchool},#{memberGradeCode},#{memberSubjectCode},#{memberRegionCode})
    </insert>

</mapper>